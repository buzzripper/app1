{
	"MicrosoftEntraID": {
		"Instance": "https://dyvenix.ciamlogin.com/",
		"Domain": "dyvenix.com",
		"TenantId": "1c3cdcca-ba60-4ad2-9892-626f5d92bc09",
		"ClientId": "dc02d6fb-755a-41b3-9bb7-ffa3acb35271",
		"ClientSecret": "<secret>",
		"ClientCapabilities": [ "cp1" ],
		"CallbackPath": "/signin-oidc",
		"SignedOutCallbackPath": "/signout-callback-oidc"
	},
	"DownstreamApi": {
		"Scopes": "api://app1-functions-gdhbavgqeyaqf2at.eastus2-01.azurewebsites.net/dc02d6fb-755a-41b3-9bb7-ffa3acb35271/access_as_user"
	},
	"ServiceClients": {
		"App1": {
			"InProcess": true,
			"Url": "https://localhost:7266"
		},
		"Auth": {
			"InProcess": true,
			"Url": "https://localhost:7174"
		}
	},
	"ReverseProxy": {
		"Routes": {
			"auth-api": {
				"ClusterId": "auth-cluster",
				"Match": {
					"Path": "/api/auth/{**catch-all}"
				},
				"Transforms": [
					{
						"PathPattern": "/api/auth/{**catch-all}"
					}
				]
			},
			"app-api": {
				"ClusterId": "app-cluster",
				"Match": {
					"Path": "/api/app/{**catch-all}"
				},
				"Transforms": [
					{
						"PathPattern": "/api/app/{**catch-all}"
					}
				]
			}
		},
		"Clusters": {
			"auth-cluster": {
				"Destinations": {
					"auth-destination": {
						"Address": "https://localhost:5002/"
					}
				}
			},
			"app-cluster": {
				"Destinations": {
					"app-destination": {
						"Address": "https://localhost:5003/"
					}
				}
			}
		}
	},
	"Logging": {
		"LogLevel": {
			"Default": "Information",
			"Microsoft": "Warning",
			"Microsoft.Hosting.Lifetime": "Information",
			"Microsoft.Identity.Web.TokenAcquisition": "Error",
			"Yarp": "Warning"
		},
		"File": {
			"Enabled": false,
			"Path": "logs/portal.log"
		}
	},
	"AllowedHosts": "*",
	"OTEL_EXPORTER_OTLP_ENDPOINT": "https://logs-prod-036.grafana.net/otlp/v1/logs",
	"OTEL_EXPORTER_OTLP_PROTOCOL": "http/protobuf",
	"OTEL_EXPORTER_OTLP_HEADERS": "<secret>",
	"OTEL_SERVICE_NAME": "portal",
	"OTEL_RESOURCE_ATTRIBUTES": "deployment.environment=dev"
